#%RAML 1.0 Trait

queryParameters: 
  page:
    description: Skip over a number of elements by specifying a page value for the query
    type: integer
    required: false
    example: 7
    default: 0
  limit:
    description: Limit the number of elements on the response
    type: integer
    required: false
    example: 80
    default: 100
